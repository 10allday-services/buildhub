{
  "name": "buildhub-ui",
  "version": "0.0.1",
  "description": "Firefox System Build info manager.",
  "scripts": {
    "build": "elm-make Main.elm --output=build/app.js && npm run copy-assets",
    "copy-assets": "copyfiles index.html style.css build/",
    "debug": "elm-live Main.elm --output=app.js --debug",
    "deploy": "npm run build && gh-pages --dist build/",
    "start": "elm-live Main.elm --output=app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leplatrem/systemaddons-versions.git"
  },
  "keywords": [
    "firefox"
  ],
  "author": "\"Storage Team\" <storage-team@mozilla.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mozilla-services/buildhub/issues"
  },
  "homepage": "https://github.com/mozilla-services/buildhub#readme",
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "elm": "^0.18.0",
    "elm-live": "^2.7.4",
    "gh-pages": "^0.12.0"
  }
}
